{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfdemopratyusha"
		},
		"Linkedsqlserver_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Linkedsqlserver'"
		},
		"SqlServerforstoredprocedure_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServerforstoredprocedure'"
		},
		"linkedservice_sql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'linkedservice_sql'"
		},
		"linkedservice_storagedemopratyusha_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedservice_storagedemopratyusha'"
		},
		"Linkedsqlserver_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "abcdsqlserver.dataset.windows.net"
		},
		"Linkedsqlserver_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "abcddevdb"
		},
		"Linkedsqlserver_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Pratchakdumdum"
		},
		"SqlServerforstoredprocedure_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "pratyusha"
		},
		"SqlServerforstoredprocedure_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dbname"
		},
		"SqlServerforstoredprocedure_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "pr"
		},
		"linkedservice_sql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "myserver.db.new"
		},
		"linkedservice_sql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().dbname}"
		},
		"linkedservice_sql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Barnachak"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfdemopratyusha_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Linkedsqlserver_password": {
						"value": "[parameters('Linkedsqlserver_password')]"
					},
					"linkedservice_sql_password": {
						"value": "[parameters('linkedservice_sql_password')]"
					},
					"linkedservice_storagedemopratyusha_connectionString": {
						"value": "[parameters('linkedservice_storagedemopratyusha_connectionString')]"
					},
					"SqlServerforstoredprocedure_password": {
						"value": "[parameters('SqlServerforstoredprocedure_password')]"
					},
					"Linkedsqlserver_properties_typeProperties_server": {
						"value": "[parameters('Linkedsqlserver_properties_typeProperties_server')]"
					},
					"Linkedsqlserver_properties_typeProperties_database": {
						"value": "[parameters('Linkedsqlserver_properties_typeProperties_database')]"
					},
					"Linkedsqlserver_properties_typeProperties_userName": {
						"value": "[parameters('Linkedsqlserver_properties_typeProperties_userName')]"
					},
					"linkedservice_sql_properties_typeProperties_server": {
						"value": "[parameters('linkedservice_sql_properties_typeProperties_server')]"
					},
					"linkedservice_sql_properties_typeProperties_database": {
						"value": "[parameters('linkedservice_sql_properties_typeProperties_database')]"
					},
					"linkedservice_sql_properties_typeProperties_userName": {
						"value": "[parameters('linkedservice_sql_properties_typeProperties_userName')]"
					},
					"SqlServerforstoredprocedure_properties_typeProperties_server": {
						"value": "[parameters('SqlServerforstoredprocedure_properties_typeProperties_server')]"
					},
					"SqlServerforstoredprocedure_properties_typeProperties_database": {
						"value": "[parameters('SqlServerforstoredprocedure_properties_typeProperties_database')]"
					},
					"SqlServerforstoredprocedure_properties_typeProperties_userName": {
						"value": "[parameters('SqlServerforstoredprocedure_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}